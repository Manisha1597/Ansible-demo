pipeline {
    agent any
    stages{
        stage('Repo Clone'){
            steps{
                git 'https://github.com/Manisha1597/Ansible-demo.git'
            }
        }
        stage('Trigger playbook') {
            steps {
                 sh "ansible-playbook Playbooks/playbook.yml"
    }
        }    
        stage('Post action') {
            steps {
                echo 'Completed'
            }
        }
    }
}
